@model Consultorio.Domain.Models.Atendimento
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Cargo</h2>
<p>
</p>
<fieldset>
    <div class="panel panel-default">
        <div class="panel-body">
            <form class="form-horizontal" role="form">
                <div class="form-group">
                    <div class="display-label">
                        <label class="col-sm-2 text-info">Código</label>
                    </div>
                    <div class="col-sm-10">
                        <label class="control-label">@Html.DisplayFor(model => model.Id)</label>
                    </div>
                </div>
                <div class="form-group">
                    <div class="display-label">
                        <label class="col-sm-2 text-info">Empresa</label>
                    </div>
                    <div class="col-sm-10">
                        <label class="control-label">@Html.DisplayFor(model => model.Empresa.Nome)</label>
                    </div>
                </div>
                <div class="form-group">
                    <div class="display-label">
                        <label class="col-sm-2 text-info">Departamento</label>
                    </div>
                    <div class="col-sm-10">
                        <label class="control-label">@Html.DisplayFor(model => model.Departamento.Descricao)</label>
                    </div>
                </div>
                <div class="form-group">
                    <div class="display-label">
                        <label class="col-sm-2 text-info">Cargo</label>
                    </div>
                    <div class="col-sm-10">
                        <label class="control-label">@Html.DisplayFor(model => model.Cargo.Descricao)</label>
                    </div>
                </div>
                <div class="form-group">
                    <div class="display-label">
                        <label class="col-sm-2 text-info">Funcionário</label>
                    </div>
                    <div class="col-sm-10">
                        <label class="control-label">@Html.DisplayFor(model => model.Funcionario.Nome)</label>
                    </div>
                </div>
                <div class="form-group">
                    <div class="display-label">
                        <label class="col-sm-2 text-info">Tipo Exame</label>
                    </div>
                    <div class="col-sm-10">
                        <label class="control-label">@Html.DisplayFor(model => model.TipoExame.Descricao)</label>
                    </div>
                </div>
                <div class="form-group">
                    <div class="display-label">
                        <label class="col-sm-2 text-info">Data</label>
                    </div>
                    <div class="col-sm-10">
                        <label class="control-label">@Html.DisplayFor(model => model.DataAtendimento)</label>
                    </div>
                </div>
                <div class="form-group">
                    <div class="display-label">
                        <label class="col-sm-2 text-info">Data Inclusão</label>
                    </div>
                    <div class="col-sm-10">
                        <label class="control-label">@Html.DisplayFor(model => model.AddedDate)</label>
                    </div>
                </div>
            </form>
        </div>
    </div>
</fieldset>
<p>
    <input type="button" class="btn btn-primary" value="Voltar" onclick="location.href='@Url.Action("Index", "Atendimento")'" />
</p>
